#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/cli/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/cli/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/rabin-wasm@0.1.5_encoding@0.1.13/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/cli/bin.js" "$@"
else
  exec node  "$basedir/../../../../../rabin-wasm@0.1.5_encoding@0.1.13/node_modules/rabin-wasm/cli/bin.js" "$@"
fi
