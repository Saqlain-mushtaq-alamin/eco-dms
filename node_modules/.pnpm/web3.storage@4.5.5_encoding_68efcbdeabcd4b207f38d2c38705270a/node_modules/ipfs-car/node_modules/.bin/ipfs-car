#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/cjs/cli/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/cjs/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/cjs/cli/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/cjs/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/dist/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules/ipfs-car/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/ipfs-car@0.7.0_encoding@0.1_dcf72fd05a3d993a817b0838ddf2dd40/node_modules:/mnt/d/canvas/eco-dsm/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cjs/cli/cli.js" "$@"
else
  exec node  "$basedir/../../dist/cjs/cli/cli.js" "$@"
fi
